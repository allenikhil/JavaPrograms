	{
		this.un=un;
		this.fn=fn;
		this.ln=ln;
		this.mn=mn;
		this.ps=ps;
		this.rps=rps;
	}
	AddUser(String un,String fn,String ln,String ps,String rps)
	{
		this.un=un;
		this.fn=fn;
		this.ln=ln;
		this.ps=ps;
		this.rps=rps;
	}
