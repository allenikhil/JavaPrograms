public class NonRepatingLetterInString {
public static void main(String[] args)
{
    String s = "mjjhfhgfjjmmn";
    for (char c : s.toCharArray())
    {
        if (s.indexOf(c) == s.lastIndexOf(c))
        {
            System.out.println("First non repeated is:" + c);
            break;
        }
    }
}
}
